{
  "name": "polkadot-wallet-generator",
  "version": "1.0.0",
  "description": "Generate Polkadot wallets and store them in PostgreSQL",
  "main": "test.js",
  "scripts": {
    "start": "node test.js",
    "test": "node test.js",
    "forge:build": "cd contracts && forge build",
    "forge:test": "cd contracts && forge test",
    "forge:test:watch": "cd contracts && forge test --watch",
    "forge:snapshot": "cd contracts && forge snapshot",
    "forge:coverage": "cd contracts && forge coverage",
    "forge:deploy": "cd contracts && forge script script/Deploy.s.sol:DeploySubscription --rpc-url $RPC_URL --broadcast"
  },
  "dependencies": {
    "@polkadot/util": "^13.5.6",
    "@polkadot/util-crypto": "^13.5.6",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/pg": "^8.10.7"
  },
  "keywords": [
    "polkadot",
    "wallet",
    "postgresql",
    "blockchain"
  ],
  "author": "Your Name",
  "license": "MIT"
}
